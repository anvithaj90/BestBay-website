<%= form_tag({:controller => "items", :action => "search_result"}, :method => "get", :id => "item_search_form", :class => "form-search") do %>
    <div class="input-append">
    <%= text_field_tag :search, params[:search], id: 'search_text', class: 'span2 search-query' %>
    <%= submit_tag "GO", :name => nil, id: 'search_submit_btn', class: 'btn'  %>
    </div>
<% end %>
