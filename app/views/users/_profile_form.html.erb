<%= form_tag({:controller => "users", :action => "profile_info"}, :method => "post") do %>

<div class="field">
  Contact Information:
  <br>
  <%= text_field_tag :contact %>
</div>

<div class="field">
  <%= file_field_tag :photo %>
</div>

<div class="actions">
  <%= submit_tag "Update" %>
</div>
<% end %>
